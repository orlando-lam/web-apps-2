<h1>New Post</h1>
<h1><%= @post["author"] %></h1>
<h2><%= @post["body"] %>, <%= @post["image"] %></h2>

<%= form_with :model => @post do |form| %>
  <p>
    <%= form.label "name", "Author" %>
    <%= form.text_field "name" %>
  </p>
  <p>
    <%= form.label "body" %>
    <%= form.text_field "body" %>
    <%= form.label "image" %>
    <%= form.text_field "image" %>
  </p>
  <%= form.submit %>
<% end %>
